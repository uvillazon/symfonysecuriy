{% extends 'ElfecSgauthBundle::logon.html.twig' %}
{% block stylesheets %}
    {#{{ parent() }}#}
    <link href="{{ asset('Content/login.css') }}" rel="stylesheet" />

{% endblock %}
{% block javascripts %}
    {{ parent() }}
    <script type="text/javascript" src={{ asset('App/View/Recuperacion/Recuperacion.js') }}></script>
    <script type="text/javascript" src={{ asset('App/Config/ux/PasswordStrength.js') }}></script>
    {#<script src="@Url.Content("~/App/Config/ux/PasswordStrength.js")" type="text/javascript"></script>#}
{% endblock %}

{% block body -%}

    <header role="banner" class="clearfix" id="header">

        <!-- start: Branding -->
        <div class="branding-elements clearfix" id="branding">

            <div id="logo">
            </div>

        </div>
        <!-- /end #branding -->


    </header>
    <footer>
        <div class="content-wrapper">
            <div class="float-left">
                <p>&copy; @DateTime.Now.Year - Sistema de Mantenimiento</p>
            </div>
        </div>
    </footer>

    <input type="hidden" id="cogioApp" name="App" value="{{ codigoApp }}">

{% endblock -%}